class Solution {
public:

    int func(int i, string &s, vector<int> &dp){
        if(i < 0){
            return 1;
        }
        if(dp[i] != -1){
            return dp[i];
        }
        int ans = 0;
        int a = s[i] - '0';
        if(a >= 1 && a <= 9){
            ans += func(i - 1, s, dp);
        }
        if(i - 1 >= 0){
            int b = (s[i - 1] - '0') * 10 + (s[i] - '0');
            if(b >= 10 && b <= 26){
                ans += func(i - 2, s, dp);
            }
        }
        return dp[i] = ans;
    }

    int numDecodings(string s) {
        int n = s.size();
        vector<int> dp(n, -1);
        return func(n - 1, s, dp);
    }
};

-----------------------------------------------------------------------

int numDecodings(string s) {
        int n = s.size();
        vector<int> dp(n + 1, 0);
        // return func(n - 1, s, dp);
        dp[0] = 1;
        for(int i = 1; i <= n; i++){
            int ans = 0;
            int a = s[i - 1] - '0';
            if(a >= 1 && a <= 9){
                ans += dp[i - 1];
            }
            if(i - 2 >= 0){
                int b = (s[i - 2] - '0') * 10 + (s[i - 1] - '0');
                if(b >= 10 && b <= 26){
                    ans += dp[i - 2];
                }
            }
            dp[i] = ans;
        }
        return dp[n];
    }
